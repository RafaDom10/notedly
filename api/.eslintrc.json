{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "no-return-await": "off",
      "prefer-destructuring": "off",
      "no-param-reassign": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "consistent-return": "off"
    }
}
